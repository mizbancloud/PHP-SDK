{
    "name": "mizbancloud/sdk",
    "description": "Official MizbanCloud SDK for PHP - CDN, Cloud, and Authentication APIs",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "MizbanCloud",
            "email": "info@mizbancloud.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "MizbanCloud\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MizbanCloud\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "keywords": [
        "mizbancloud",
        "cdn",
        "cloud",
        "iaas",
        "sdk",
        "api"
    ]
}
